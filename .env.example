# Anthropic API Key (必需)
ANTHROPIC_API_KEY=your_api_key_here

# 模型配置
DEFAULT_MODEL=claude-sonnet-4-20250514
MAX_TOKENS=8192

# Agent配置
AGENT_NAME=MyAgent
MAX_ITERATIONS=100
AUTO_CONFIRM=false

# 数据库路径
DATABASE_PATH=data/agent.db

# 日志级别
LOG_LEVEL=INFO

# GitHub Token (用于搜索和下载技能)
GITHUB_TOKEN=your_github_token_here
